<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>RegistrarReserva</servlet-name>
        <servlet-class>Controller.Registrar_Reserva</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Listar_Reservas</servlet-name>
        <servlet-class>Controller.Listar_Reservas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Listar_cabanas</servlet-name>
        <servlet-class>Controller.Listar_cabanas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrarReserva</servlet-name>
        <url-pattern>/RegistrarReserva</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Listar_Reservas</servlet-name>
        <url-pattern>/Listar_Reservas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Listar_cabanas</servlet-name>
        <url-pattern>/Listar_cabanas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
